@model DateTime

@{
    string keyName = nameof(DancingGoat.Helpers.Extensions.DateTimeFormatterParameters.FormatCharacter);

    if (ViewData.ContainsKey(keyName) && !string.IsNullOrEmpty(ViewData[keyName].ToString()))
    {
        Output.Write(Model.ToString(ViewData[keyName].ToString()));
    }
    else
    {
        @ViewData.TemplateInfo.FormattedModelValue
    }
}